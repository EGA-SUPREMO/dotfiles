git filter-branch -f  --index-filter \
    'git rm --force --cached --ignore-unmatch $1' \
     -- --all

git filter-branch --index-filter "git rm -rf --cached --ignore-unmatch $1" HEAD
     
  rm -Rf .git/refs/original && \
    git reflog expire --expire=now --all && \
    git gc --aggressive && \
